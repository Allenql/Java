package com.lz.schoolMS.ds;

public class LinkList {
	private Node head;
	private int length;
	
	public void add(int data){
		Node n = new Node(data);
		if(head == null){
			head = n;
			length++;
		}
		Node tmp = head;
		while(tmp.next != null){
			tmp = tmp.next;
		}
		tmp.next = n;
		length++;
	}

}
